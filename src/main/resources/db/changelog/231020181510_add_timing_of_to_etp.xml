<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="231020181510" author="Vlad">

        <addColumn tableName="etp">
            <column name="begin_date" type="java.sql.Types.TIMESTAMP"></column>
            <column name="end_date" type="java.sql.Types.TIMESTAMP"></column>
        </addColumn>

        <rollback>
            <dropColumn tableName="etp">
                <column name="begin_date" type="java.sql.Types.TIMESTAMP"></column>
                <column name="end_date" type="java.sql.Types.TIMESTAMP"></column>
            </dropColumn>
        </rollback>

    </changeSet>
</databaseChangeLog>
